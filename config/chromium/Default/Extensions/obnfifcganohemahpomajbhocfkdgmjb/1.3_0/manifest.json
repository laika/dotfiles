{
   "background": {
      "scripts": [ "/js/background.js" ]
   },
   "browser_action": {
      "default_icon": "/img/icon-48.png",
      "default_popup": "/html/popup.html"
   },
   "content_scripts": [ {
      "js": [ "/js/inject_skip.js" ],
      "matches": [ "*://*.adf.ly/*", "*://*.j.gs/*", "*://*.q.gs/*", "*://*.ay.gy/*" ]
   } ],
