   "icons": {
      "128": "/img/icon-128.png",
      "16": "/img/icon-16.png",
      "48": "/img/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "AdF.ly Skipper ★WORKING★",
   "short_name": "AdF.ly Skipper",
   "update_url": "https://clients2.google.com/service/update2/crx",
